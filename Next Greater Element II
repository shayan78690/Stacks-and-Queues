







class Solution {

    public int[] nextGreaterElements(int[] nums) {
        int n = nums.length;
        int result[] = new int[n];
        for(int i = 0; i < n; i++) {
            result[i] = -1;
        }

        for (int i = 0; i < n; i++) {
            for (int j = i + 1; j < i + n; j++) {
                if (nums[i] < nums[j % n]) {
                    result[i] = nums[j % n];
                    break;
                }
            }
        }
        return result;
    }
}
